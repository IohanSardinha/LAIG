            <diffuse r="0.1" g="0.1" b="0.1" a="1" />
            <specular r="0.1" g="0.1" b="0.1" a="1" />
            <emissive r="0" g="0" b="0" a="1" />
        </material>
        
        <material id="transparentMaterial">
