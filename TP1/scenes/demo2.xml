<lsf>
    <initials>
        <reference length="0" />
        <!-- axis length; "0" means no axis displayed -->
        <root id="root" />
    </initials>
    <views default="defaultCamera">
        <!-- at least one of the following type of cameras must be declared -->
        <perspective id="defaultCamera" near="0.1" far="1000" angle="40">
            <from x="5" y="7" z="17" />
            <to x="-2" y="4" z="-2" />
        </perspective>
        <perspective id="frontCamera" near="0.1" far="1000" angle="90">
            <from x="0" y="5" z="10" />
            <to x="0" y="4" z="0" />
        </perspective>
        <perspective id="topCamera" near="0.1" far="1000" angle="90">
            <from x="0" y="12.5" z="0" />
            <to x="-1" y="0" z="0" />
        </perspective>
        <perspective id="couchCamera" near="0.1" far="1000" angle="90">
            <from x="-6" y="5" z="0" />
            <to x="-1" y="5" z="0" />
        </perspective>
        <perspective id="tvCamera" near="0.1" far="1000" angle="90">
            <from x="6" y="5" z="0" />
            <to x="-1" y="5" z="0" />
        </perspective>
         <ortho id="orthoCamera" near="0.1" far="100" left="-20" right="20" top="18" bottom="-18">
            <from x="-10" y="15" z="90" />
            <to x="0" y="-2" z="0" />
            <up x="0" y="1" z="0" />
        </ortho>


        
    </views>

    <illumination>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />           <!-- global ambient -->
        <background r="0.11" g="0.11" b="0.11" a="1.0" />        <!-- background color -->
    </illumination>


    <lights>
        <!-- NOTE: this block "light" must be repeated as necessary with different "id". At least one light should be present, and a maximum of eight lights is accepted. -->
        <light id="lamp">            <!-- light identifier -->
            <enable value="1" />
            <!-- enable/disable -->
            <position x="-5.2" y="5.5" z="6" w="1.0" /><!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />                   <!-- ambient component -->
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />             <!-- diffuse component -->
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />            <!-- specular component -->
        </light>
        <light id="lamp2">            <!-- light identifier -->
            <enable value="1" />
            <!-- enable/disable -->
            <position x="-5.2" y="5.5" z="4" w="1.0" /><!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />                   <!-- ambient component -->
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />             <!-- diffuse component -->
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />            <!-- specular component -->
        </light>
        <light id="top">            <!-- light identifier -->
            <enable value="1" />
            <!-- enable/disable -->
            <position x="0" y="10" z="0.0" w="1.0" /><!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0" />                   <!-- ambient component -->
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />             <!-- diffuse component -->
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />            <!-- specular component -->
        </light>
        <light id="behindTV">            <!-- light identifier -->
            <enable value="1" />
            <!-- enable/disable -->
            <position x="6.5" y="5" z="0" w="1.0" /><!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0" />                   <!-- ambient component -->
            <diffuse r="0.87" g="1.0" b="1.0" a="1.0" />             <!-- diffuse component -->
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />            <!-- specular component -->
        </light>
        <!--light id="window">        
            <enable value="1" />
            <position x="4" y="7.5" z="-5" w="1.0"/>
            <ambient r="0" g="0" b="0" a="1.0" />                  
            <diffuse r="0.98" g="0.98" b="0.82" a="1.0" />           
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />      
        </light-->
    </lights>


